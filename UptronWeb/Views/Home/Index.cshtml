                                     data-speed="1000"
                                     data-start="2000"
                                     data-easing="Back.easeInOut"
                                     data-endspeed="300"
                                     style="z-index:7; color: #fff; font-size: 26px;font-family: Noto Sans; letter-spacing: 1px; ">
                                    @slider.Caption1
                                </div>

                                <div class="tp-caption mediumlarge_light_white_center customin customout start hidden-xs"
