@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<form>
    <section class="content-wrapper" role="main">
        <div class="content">
            <div class="content-bar">
                <ul class="breadcrumb breadcrumb-angle">
                    <li><a href="#" aria-label="home"><i class="fa fa-home"></i></a></li>
                    <li class="active">Our Partner's</li>
                </ul>
            </div><!-- /.content-bar -->
            <hr />

            <div class="content-body">
                <div class="col-lg-12">
                    <div class="featured-box featured-box-primary align-left mt-xlg">
                        <div class="box-content">
                            <div class="row">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-12"><h4 class="heading-primary text-uppercase mb-md">Partner's</h4></div>
                                    </div>
                                    <hr class="tall">
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <div class="form-group mt-lg">
                                                    <label class="col-sm-2 control-label">Partner's Name</label>
                                                    <div class="col-sm-10">
                                                        <div class="input-group input-group-in">
                                                            <input type="text" id="Partnername" name="Partnername" class="form-control" placeholder="Partner's Name" required />
                                                            <span class="input-group-addon">
                                                                <span class="icon"><i class="fa fa-user-secret"></i></span>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-6">
                                                <div class="form-group mt-lg">
                                                    <div class="form-group">
                                                        <div class="form-group mt-lg">
                                                            <label class="col-sm-4 control-label">Upload Image </label>
                                                            <div class="col-sm-8">
                                                                <div class="input-group input-group-icon">
                                                                    <input type="file" name="FunctionariesFile" id="GOFile" required />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <br />
                                                <div class="btn-divider mr-2x mb-1x" role="divider">
                                                    <input id="btnSave" type="submit" value="Submit" name="submit" class="btn btn-primary">
                                                    <span class="label-divider" id="Span1">or</span>
                                                    <a href="" class="btn btn-default">Reset</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                </div>

                <div class="panel" data-fill-color="true">
                    <div class="panel-body">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="table-responsive">
                                    <table class="table table-bordered">
                                        <thead>
                                            <tr class="bg-primary">
                                                <th colspan="7">Partner's Detail</th>
                                            </tr>
                                            <tr class="active">
                                                <th style="width:10%">Sl.no</th>
                                                <th style="width:30%">Partner Name</th>
                                                <th style="width:30%">Partner Image</th>
                                                <th style="width:20%">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td style="width:10%">
                                                    <div class="form-group mt-lg">
                                                        <div class="input-group input-group-icon">
                                                            <span class="headline center"><strong>1</strong></span>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td style="width:30%">
                                                    <div class="form-group mt-lg">
                                                        <div class="input-group input-group-icon">
                                                            <span class="headline center">BSNL Connecting</span>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td style="width:30%">
                                                    <div class="form-group mt-lg">
                                                        <div class="input-group input-group-icon">
                                                            <span class="headline center">
                                                                <div class="partnersLogo clearfix">
                                                                    <img src="~/img/home/brandn-10.jpg" alt="Image Partner" />
                                                                </div>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td style="width:20%">
                                                    <div class="form-group mt-lg">
                                                        <div class="btn-divider mr-2x mb-1x" role="divider">
                                                            <a href="#" class="btn btn-primary">Edit</a>
                                                            <span class="label-divider" id="Span1">or</span>
                                                            <a href="#" class="btn btn-danger">Delete</a>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div><!-- /.panel -->
                    <div class="container">
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div><!-- /.content -->
    </section>
</form>
